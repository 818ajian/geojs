{
  "opts": {
    "template": "node_modules/jaguarjs-jsdoc"
  },
  "plugins": [
    "jsdoc/plugins/events",
    "jsdoc/plugins/typedef_augments",
    "node_modules/jsdoc-autoprivate/autoprivate.js",
    "plugins/markdown"
  ],
  "templates": {
    "applicationName": "GeoJS",
    "linenums": true,
    "cleverLinks": true,
    "default": {
      "outputSourceFiles": true
    }
  }
}
